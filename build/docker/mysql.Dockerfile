FROM mysql:8.0 as local_db

ARG MYSQL_PROXY_HOST
ENV MYSQL_PROXY_HOST $MYSQL_PROXY_HOST
ARG MYSQL_HOST
ENV MYSQL_HOST $MYSQL_HOST
ARG MYSQL_DATABASE
ENV MYSQL_DATABASE $MYSQL_DATABASE
ARG MYSQL_USER
ENV MYSQL_USER $MYSQL_USER
ARG MYSQL_PASSWORD
ENV MYSQL_PASSWORD $MYSQL_PASSWORD
ARG MYSQL_PASSWORD
ENV MYSQL_PASSWORD $MYSQL_PASSWORD
ARG MYSQL_PORT
ENV MYSQL_PORT $MYSQL_PORT
ARG TZ
ENV TZ $TZ
